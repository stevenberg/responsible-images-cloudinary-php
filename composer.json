{
  "name": "stevenberg/responsible-images-cloudinary",
  "type": "library",
  "description": "Cloudinary URL maker for stevenberg/responsible-images",
  "keywords": [
    "images",
    "responsive"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/stevenberg/responsible-images-cloudinary-php/issues",
    "source": "https://github.com/stevenberg/responsible-images-cloudinary-php"
  },
  "authors": [
    {
      "name": "Steven Berg",
      "email": "steven@stevenberg.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "cloudinary/cloudinary_php": "^2.14",
    "stevenberg/responsible-images": "^5.0.0"
  },
  "require-dev": {
    "laravel/pint": "^1.17.1",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^1.11.8",
    "phpstan/phpstan-deprecation-rules": "^1.2",
    "phpstan/phpstan-phpunit": "^1.4",
    "phpstan/phpstan-strict-rules": "^1.6",
    "phpunit/phpunit": "^11.2.9",
    "psy/psysh": "^0.12.4",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "StevenBerg\\ResponsibleImages\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StevenBerg\\ResponsibleImages\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "archive": {
    "exclude": [
      "/tests"
    ]
  }
}
